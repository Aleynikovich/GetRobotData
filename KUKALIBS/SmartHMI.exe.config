  <!-- ### UIFramework Configuration -->
  <KukaRoboter.SmartHMI.UIFrameworkConfiguration type="KukaRoboter.SmartHMI.UIFramework.Configuration.UIFrameworkConfiguration, KukaRoboter.SmartHMI.UIFrameworkServices">
    <!-- ApplicationFrame: Standard / KcpSimulation / None / Auto
         StartupChecks: Never / ConfigChanged / Always -->
    <UIFrameworkConfiguration
       ApplicationFrame="Auto" DesktopAccess="Auto" StartupChecks="ConfigChanged" LoggedExceptionsLimit="1000"
       CheckInstallationType="true">
      <!-- Exception rule: After Startup, log all exceptions but do not show dialogs -->
      <ExceptionRule SystemName="NoDialogsAfterStartup" Type="System.Exception" IncludeSubclasses="true"
                     Phases="Run|Shutdown" ShowDialog="Never" Log="Always"/>-->
      <!-- Supported Languages -->
      <Language Culture="de-DE" />
      <Language Culture="en-US" />
      <Language Culture="fr-FR" />
      <Language Culture="it-IT" />
      <Language Culture="es-ES" />
      <Language Culture="pt-PT" />
      <Language Culture="nl-NL" />
      <Language Culture="pl-PL" />
      <Language Culture="sv-SE" />
      <Language Culture="da-DK" />
      <Language Culture="cs-CZ" />
      <Language Culture="ru-RU" />
      <Language Culture="zh-CN" />
      <Language Culture="fi-FI" />
      <Language Culture="sl-SI" />
      <Language Culture="hu-HU" />
      <Language Culture="sk-SK" />
      <Language Culture="ja-JP" />
      <Language Culture="ko-KR" />
      <Language Culture="tr-TR" />
      <Language Culture="ro-RO" />
      <Language Culture="el-GR" />
      <!-- BusyBox configuration (Time syntax "hh:mm:ss.ss")-->
      <BusyBox Enabled="true" Delay="0:0:1.5" MinShowTime="0:0:0.75" WarnBlockTime="0:0:15"/>
      <!-- Application watchdog configuration -->
      <Watchdog Enabled="true" MaxBlockTime="0:01:00" SignalAliveInterval="0:0:5" />
      <!-- KXR modules to be preloaded of startup -->
      <PreloadKxrModules>
        Jogging, StatusBarElements, LegacySupport, Cross3Archive, Navigator, RobotData,
        SoftKeyBar, MenuBar, CrossMeld, Common, PMService, SmartHMI
      </PreloadKxrModules>
    </UIFrameworkConfiguration>
  </KukaRoboter.SmartHMI.UIFrameworkConfiguration>

